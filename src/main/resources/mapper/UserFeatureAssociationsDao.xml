<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.user.UserFeatureAssociationsDao">

    <!--添加用户功能映射id-->
    <insert id="addFunctionId" useGeneratedKeys="true">
        INSERT INTO `picture-editor`.`user_feature_associations`
            (openId,userVipId,userPictureId,userApiId)
        VALUES
            (#{openId}, #{userVipId}, #{userPictureId}, #{userApiId});
    </insert>
</mapper>
